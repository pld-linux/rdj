diff -u -N -r rdj-0.3.2/configure.in rdj-0.3.2.patch/configure.in
--- rdj-0.3.2/configure.in	Thu Jan  1 01:00:00 1970
+++ rdj-0.3.2.patch/configure.in	Tue Apr  2 18:29:08 2002
@@ -0,0 +1,51 @@
+# Process this file with autoconf to produce a configure script.
+AC_INIT(src)
+AM_INIT_AUTOMAKE(rdj, 0.3.2)
+
+# Checks for programs.
+AC_PROG_CXX
+AC_PROG_CC
+AC_PROG_INSTALL
+
+
+# Checks for header files.
+AC_HEADER_STDC
+AC_HEADER_SYS_WAIT
+AC_CHECK_HEADERS([errno.h fcntl.h locale.h stdlib.h string.h sys/ioctl.h unistd.h])
+
+# Checks for typedefs, structures, and compiler characteristics.
+AC_C_CONST
+
+AC_CHECK_PROG(GTK_CFLAGS, gtk-config, `gtk-config --cflags`,,,)
+AC_CHECK_PROG(GTK_LIBS, gtk-config, `gtk-config --libs`,,,)
+
+AC_CHECK_PROG(GNOME_CFLAGS, gnome-config, `gnome-config --cflags gnomeui`,,,)
+AC_CHECK_PROG(GNOME_LIBS, gnome-config, `gnome-config --libs gnomeui`,,,)
+
+
+# saving cflags copy, so all configure tests won't use them (faster)
+CFLAGS_c=$CFLAGS
+LIBS_c=$LIBS
+CFLAGS="$GTK_CFLAGS $GNOME_CFLAGS"
+LIBS="$GTK_LIBS $GNOME_LIBS"
+
+AC_CHECK_LIB(gtk,gtk_init,,AC_MSG_ERROR([***  gtk+ not found  ***]))
+AC_CHECK_LIB(gnome,gnome_color_picker_new,,AC_MSG_ERROR([*** gnome-libs not found ***]))
+
+CFLAGS=$CFLAGS_c
+LIBS=$LIBS_c
+
+# gettext
+ALL_LINGUAS="cs de es it nl ru tr"
+AM_GNU_GETTEXT
+
+# Checks for library functions.
+AC_PROG_GCC_TRADITIONAL
+AC_FUNC_MALLOC
+AC_TYPE_SIGNAL
+AC_CHECK_FUNCS([memset setlocale strcasecmp strchr strerror])
+
+AC_DEFINE(_GNU_SOURCE)
+
+AC_OUTPUT([Makefile intl/Makefile po/Makefile.in src/Makefile])
+
